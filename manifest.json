{
"update_url":"http://clients2.google.com/service/update2/crx",
	"name": "Amazon Monetizer",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Turn all your Amazon purchases into Amazon affiliate purchases.",
	"browser_action": {
		"default_icon": "elaine.png",
		"default_title": "Add amazon affiliate.",
		"default_popup": "amzn.html"
	},
	"permissions": ["webRequest", "webRequestBlocking", 
					"http://www.amazon.com/*", "tabs"],
	"background": {
		"scripts": ["request.js"]
	}
}